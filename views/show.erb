<h2>Admin show page</h2>
<%= @db_result %>

<table class="table">
  <thead>
    <tr>
      <th scope="col">user_name</th>
      <th scope="col">user_phone</th>
      <th scope="col">date_time</th>
      <th scope="col">select_teacher</th>
      <th scope="col">color_jquery</th>
    </tr>
  </thead>
  <body>
    <% @db_result.each do |item| %>
        <tr>
          <td><%= item['user_name'] %></td> 
          <td><%= item['user_phone'] %></td> 
          <td><%= item['date_time'] %></td>  
          <td><%= item['select_teacher'] %></td> 
          <td><%= item['color_jquery'] %></td> 
        </tr>
    <% end %>
  </body>
</table>